     <script type="text/javascript">
		var Host={CAS:"https:\/\/passport.1919.cn\/"||'',VENDOR:"https:\/\/vendor.1919.cn\/"||'',UI:"https:\/\/www.1919.cn\/"||'',IMGURL:"https:\/\/1919-new-bbc-pro.oss-cn-beijing.aliyuncs.com\/"||''};
	    var isLogined=false,geoProvincesOfMapInitFinished=false,lastData={currentCity:'成都'};
	    void function(){
	    	//判断是否显示导航菜单品牌商城
	    	$.ajax({
	    		type:'post',
	    		url:Host.UI+'isMall',
	    		dataType:'json',
	    		data:{},
	    		success:function(result){
	    			var $navMall=$('#navMall');
	    			if (result.code === 0 && result.data) {
	    				$navMall.css('display','block');
	    			} else {
	    				$navMall.css('display','none');
	    			}
	    		}
	    	});
	        var host=window.location.href;
	        var loginURL=Host.CAS+'login.do?redirectURL='+host;
	        var regURL=Host.CAS+'register.do';
	        var logoutURL=Host.UI+'my/logout.do';
	        $.ajax({
	        	type:'post',
	        	url:Host.UI+'my/user_info.do',
	            cache:false, 
	            async:false, 
	        	dataType:'json',
	        	success:function(result){
	        		if(result.code === 0){
                        var nickName = result.data.nickName;
                        //如果用户名为手机号的话，将手机号中间数字隐藏
                        var mobileReg = /0?(13|14|15|18|17)[0-9]{9}/;
                        if(mobileReg.test(nickName)){
							nickName = nickName.replace(/(\d{3})\d{4}(\d{4})/, '$1****$2');
						}
                        var html='<span>欢迎光临1919酒类直供！</span><a href="'+Host.CAS+'" target="_blank"><span class="mc-uname">'+ nickName+'</span>&nbsp;<span>('+result.data.level+')</span></a><a href="'+logoutURL+'">[退出]</a>';
			            $("#userInfo").html(html);
			            $('#myCenter').html('<a class="mc-acc" href="'+Host.CAS+'" target="_blank">我的1919</a>');
			            if(result.data.isVendor){
			                $("#vendor-register-link").remove();
			            } else {
			                $("#vendor-center-link").remove();
			            }
			            $('.logined').show();
			            lastData.currentCity=result.data.cityName||'成都';
			            isLogined = true;
	        		}else{
	                    var html='<span>欢迎光临1919酒类直供！</span><span class="mc-login"><a href="'+loginURL+'" target="_blank">亲，请登录</a></span><span class="mc-signup"><a href="'+regURL+'" target="_blank">去注册</a></span>';
			            $("#notLogin").html(html);
			            $("#vendor-center-link").remove();
	        		}
	        	}
	        });        
	        $.get(Host.UI+'geo/deliveryCity.do',function(result){
	            $('#cityName').each(function(){
	                result = result || '全国';
	                $(this).html(result);
	                window.userLocation = result;
	                $(document).trigger('locationUpdate',result);
	            });
	        });
	        $('#vendor-center-link a').attr('href', Host.VENDOR+"index.html");
	    }();
	</script> 
