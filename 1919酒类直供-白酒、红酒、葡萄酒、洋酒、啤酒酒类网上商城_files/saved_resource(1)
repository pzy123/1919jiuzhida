       <script type="text/javascript">
var applySearch=function(){
    $('#searchForm').submit();
};
$(function(){
	var defaultKey="";
	$.ajax({
		type:'post',
		url:Host.UI+'/defaultKeyword.do',
		data:{},
		dataType:'json',
		success:function(data){
            if (!data.code && data.data) {
                var skeyword = data.data;
                if (skeyword && skeyword.keywordPlaceholder) {
                    defaultKey = skeyword.keywordPlaceholder;
                    var $kw = $('#kw');
                    var kVal = $.trim($kw.val())||defaultKey;
                    $kw.val(kVal);
                }
            }
		}
	});
    $("#kw").focus(function(){
  	   if($(this).val()==defaultKey){
  		   $(this).val("");
  	   }
    }).blur(function(){
      	if($(this).val()==""){
      		$(this).val(defaultKey);
      	}
    });
    $.ajax({
        type:'get',
        url:Host.UI+'/covers/programs/tscp',
        data:{},
        dataType:'json',
        success:function(data){
            if (!data) {return;}
            var list=data.children.hk.elements;
            if (list.length) {
			    var hk_li='<li class="u-mainRedColor">热门搜索 :</li>';
	            for (var i=0,len=list.length;i<len;i++){
	            	hk_li+='<li><a class="'+list[i].styleClass+'" href="'+list[i].link+'" target="_blank">'+list[i].text+'</a></li>';
	            }
	            $('#hkList').html(hk_li);
            }
        }
    });
});
$(document).ready(function(){
	$('#sleHid>dl').on({
		mouseover:function(){
			$(this).stop(true).find('dd').show();
			return false;
		},
		mouseout:function(){
			$(this).stop(true).find('dd').hide();
			return false;
		}
	}).find('dd>a').on('click',function(){
		var $that=$(this);
		$that.addClass('selected').siblings().removeClass('selected');
		$that.parent().prev().find('label').html(this.name);
		$that.parent().prev().prev().val(this.id);
		var $searchForm=$('#searchForm');
		if(this.id=='p'){
			$searchForm.prop('action',Host.UI+'search.html');
		}else{
			$searchForm.prop('action',Host.UI+'vendorSearch.html');
		}
	});
});
</script>
