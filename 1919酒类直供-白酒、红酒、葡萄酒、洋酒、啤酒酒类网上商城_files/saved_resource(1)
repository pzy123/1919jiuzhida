     <script type="application/javascript">
		$(function(){
		    var len = $("li.bran-showcase-toggle").length,
				arr = [],
				i;
		    for(i=0; i<len; i++){
				arr[i] = 0;
			}
               $("body").on("click", ".refresh-brand-showcase-btn", function (){
                   var $this = $(this),
					_index = $this.closest(".br-cont-box").index()-1,
					categoryId = $this.data("category");
                   refreshBrandShowcaseElements(categoryId, arr[_index], _index);
               });
               function refreshBrandShowcaseElements(categoryId, num, _index){

                   var fixedCategoryId = categoryId.replace(/\D/, "");
                   $.getJSON(Host.UI + "brandShowcase/listByCategory.do?categoryId=" + fixedCategoryId + "&pageNo=" + num, function (result) {
                       var $branShowcaseContainer = $(".branShowcase-container" + fixedCategoryId);
                       if(result.brandAndVendorInfoVos && result.brandAndVendorInfoVos.length){

                           var sourceBrandShowcase = $(".brand-showcase-template").html();
                           var brandShowcaseElementHtmls = "";
                           for(var branShowcaseIndex in result.brandAndVendorInfoVos){
                               var brandShowcase = result.brandAndVendorInfoVos[branShowcaseIndex];
                               brandShowcaseElementHtmls += sourceBrandShowcase.replace(/#vendorLink#/g, brandShowcase.vendorLink).replace(/#vendorLogoImgUrl#/g, brandShowcase.vendorLogoImgUrl || brandShowcase.brandLogoImgUrl).replace(/#vendorName#/g, brandShowcase.vendorName || brandShowcase.brandName);
                           }

                           $branShowcaseContainer.find("li.br-cont-rgi").each(function(){
                               var $showcase = $(this);
                               if(!$showcase.hasClass("br-change")){
                                   $showcase.remove();
                               }
                           });
                           if(result.count <=18){
                               $branShowcaseContainer.find(".br-change").remove();
                           }else{
                               arr[_index] += 17;
                           }
                           $branShowcaseContainer.find(".br-cont-list").prepend(brandShowcaseElementHtmls);
                       }else{
                           $branShowcaseContainer.find(".br-change").remove();
					}

                   });
               }
			$("li.bran-showcase-toggle").each(function(){
			    var $this = $(this),
					_index = $this.index();
				refreshBrandShowcaseElements($this.data("id"), arr[_index], _index);
			});
            $('#news_top').load("../news/top/4");
		});
	</script> 
    </div> 
