    <script type="text/javascript">
	//------------------加载购物车下拉框（将购物车一堆加载到右上角）--------------------
	var initPanel = function(){
		$.get(Host.UI + 'my/cart/panel.do',function(data){
		   if(typeof data === 'string'){
               $('.u-car').html(data);
		   }
		});
	};
	//-----------------删除指定商品-----------------
    $('.u-car').on('click', '#del-item', function () {
        var li = $(this).closest('li');
        pCode = li.attr('data-id');
        if (pCode) {
            $.ajax({
                url: Host.UI + '/my/cart/rm.do',
                type: 'GET',
				dataType: 'json',
                data: {pCodes: '' + pCode}
            }).success(function (result) {
                if (result && result.code == 0) {
                    initPanel();
                } else {
                    layer.msg("删除失败");
                }
            }).error(function () {
                layer.msg("删除失败");
            });
        }
    });
	$(function(){
		initPanel();
		$('#favorite').on('click',function(){
			var that=this,$that=$(that);
			$.ajax({
				type:'post',
				url:Host.UI+'my/favoriteVendor/'+that.name,
				dataType:'json',
				success:function(data){
                    if(data && data.code === 727){
                    	window.location.href=data.data;
                    }else{
                    	var $i=$that.find('i');
                    	var $span=$that.find('span');
						if(data && data.code == 0) {
							$i.removeClass('icon-nocollect').addClass('icon-collect');
							$that.find('span').html('已收藏');
						} else {
							$i.removeClass('icon-collect').addClass('icon-nocollect');
							$span.html('收藏');
						}
                    }
				}
			});
			return false;
		});
	});
</script>
